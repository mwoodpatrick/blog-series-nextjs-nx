{
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "affected": {
    "defaultBase": "master"
  },
  "npmScope": "juridev",
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "build-storybook"
        ]
      }
    }
  },
  "projects": {
    "markdown": {
      "tags": []
    },
    "react-app": {
      "tags": []
    },
    "react-app-e2e": {
      "tags": [],
      "implicitDependencies": ["react-app"]
    },
    "reactui": {
      "tags": []
    },
    "shared-mdx-elements": {
      "tags": []
    },
    "shared-ui": {
      "tags": []
    },
    "site": {
      "tags": [],
      "implicitDependencies": ["site-articles"]
    },
    "site-articles": {
      "tags": []
    },
    "site-e2e": {
      "tags": [],
      "implicitDependencies": ["site"]
    },
    "storybook-e2e-ui-e2e": {
      "tags": [],
      "implicitDependencies": ["shared-ui"]
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  }
}
